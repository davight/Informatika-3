InlayHints:
  ParameterNames: false
  DeducedTypes: false

Completion:
  AllScopes: No
  ArgumentLists: Delimiters
  HeaderInsertion: Never
  CodePatterns: None

CompileFlags:
  Add:
    - "--target=x86_64-w64-windows-gnu"
    - "-stdlib=libstdc++"
    - "-nostdinc++"
    - "-isystemC:/msys64/ucrt64/include/c++/15.2.0"
    - "-isystemC:/msys64/ucrt64/include/c++/15.2.0/x86_64-w64-mingw32"
    - "-isystemC:/msys64/ucrt64/include/c++/15.2.0/backward"
    - "-isystemC:/msys64/ucrt64/lib/gcc/x86_64-w64-mingw32/15.2.0/include"
    - "-isystemC:/msys64/ucrt64/include"
    - "-isystemC:/msys64/ucrt64/lib/gcc/x86_64-w64-mingw32/15.2.0/include-fixed"
    - "-fdiagnostics-color=always"

Diagnostics:
  Suppress:
    - ".*__rdtsc.*"
